<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.crm.coursetype.domain.CrmCourseType" table="crm_coursetype">
		<id name="courseTypeId"">
			<generate class="uuid"></generate>
		</id>
		<property name="courseCost"></property>	
		<property name="total"></property>	
		<property name="courseName"></property>	
		<property name="remark"></property>	
		
	</class>
</hibernate-mapping>
